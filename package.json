{
  "name": "assets-loading-tracker",
  "version": "1.1.1",
  "description": "A zero-config React hook to automatically track and display loading progress for images, videos, audios, and fonts with TypeScript support.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "hooks",
    "loading",
    "progress",
    "assets",
    "preloader",
    "typescript",
    "images",
    "fonts",
    "videos"
  ],
  "bugs": {
    "url": "https://github.com/riidha-bennafla/assets-loading-tracker/issues"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "author": "Bennafla Ridha <bennafla.riidha@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/riidha-bennafla/assets-loading-tracker",
  "repository": {
    "type": "git",
    "url": "https://github.com/riidha-bennafla/assets-loading-tracker.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "typescript": "^5.6.0"
  }
}
